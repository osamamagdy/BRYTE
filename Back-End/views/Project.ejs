                      <stop offset="0.190625" stop-color="#4B90F7" />
                      <stop offset="1" stop-color="#4B90F7" stop-opacity="0" />
                    </linearGradient>
                  </defs>
                </svg>
                <pre>
  <%= project_container.project.PRO_Description %>
                </pre>
              </div>
            </div>
          </div>

