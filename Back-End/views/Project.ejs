                          <% }%>

                      <span
                        >| by:
                        <a href="" style="color: #66a3ff"><%=project_container.reviews[x].EXP_Fname %> <%=project_container.reviews[x].EXP_Lname %></a></span
                      >
                    </div>
                  </div>
                  <div class="review-date"><%=project_container.reviews[x].REV_Date %></div>
                </div>
                <div>
                  <div class="review-tail">
                    <%=project_container.reviews[x].REV_Description %>
                  </div>
                </div>
              </div>
              <% } %>
            </div>
            </div>
          </div>
          <% }%>
        </div>
