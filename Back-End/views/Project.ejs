              <div class="phasedate-list">
                <% for(x in project_container.timeline) {%>
                  <div class="phase-date"><%= project_container.timeline[x].PRO_TIM_Phase_Start_Date %> - <%= project_container.timeline[x].PRO_TIM_Phase_End_Date %></div>
               <% } %>
              </div>
