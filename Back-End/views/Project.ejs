<%- include('partials/header'); -%>
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Project</title>
  </head>

  <body>
    <div class="main">
      <div id="side-bar-container">
        <div class="side_bar">
          <div id="upper">
            <svg
              width="197"
              height="144"
              viewBox="0 0 197 144"
              fill="none"
              xmlns="http://www.w3.org/2000/svg"
            >
              <path
                d="M139.937 110.353L181 76.5216L141.22 45.9126M57.0633 110.353L16 76.5216L55.7801 45.9126M76.4213 134L128.408 10"
                stroke="#48CC75"
                stroke-width="19"
                stroke-linecap="round"
              />
            </svg>
            <h1>Personal Project</h1>
          </div>
          <svg
            id="side-bar-arrow"
            width="313"
            height="119"
            viewBox="0 0 313 119"
            fill="none"
            xmlns="http://www.w3.org/2000/svg"
          >
            <path
              d="M278 12H19.3309C9.20702 12 1 20.0589 1 30V119"
              stroke="url(#paint0_linear)"
              stroke-width="2"
            />
            <circle
              cx="288.5"
              cy="11.5"
              r="11.5"
              fill="#020f22"
              stroke-width="2"
            />
            <circle
              cx="288.5"
              cy="11.5"
              r="5.5"
              transform="rotate(90 288.5 11.5)"
              stroke="white"
              stroke-width="2"
            />
            <path
              d="M296 22L309 11.5L296.406 2"
              stroke="white"
              stroke-width="4"
              stroke-linecap="round"
            />
            <defs>
              <linearGradient
                id="paint0_linear"
                x1="274.945"
                y1="12"
                x2="5.87031"
                y2="118.306"
                gradientUnits="userSpaceOnUse"
              >
                <stop stop-color="white" />
                <stop offset="1" stop-color="#4B90F6" stop-opacity="0" />
              </linearGradient>
            </defs>
          </svg>
          <div id="developers-container">
            <h2>Developers:</h2>
            <div id="developers-bar">
            <% for(x in project_container.devs) { %>  
              <a href="/developer/<%= project_container.devs[x].DEV_ID %>">
              <div class="review-head dev">
                <img onerror="this.onerror=null;this.src='../assets/img placeholder.svg';" class="review-avatar dev" src="<%= project_container.devs[x].DEV_Profile_Picture %>" />
                <div class="reviewer-cont dev">
                  <a href="#" class="review-title dev"><%= project_container.devs[x].DEV_Fname %> <%= project_container.devs[x].DEV_Lname %></a>
                  <div class="reviewer dev" style="font-family: sans-serif">
                    <span style="color: #66a3ff"><%= project_container.devs[x].DEV_Role %></span>
                  </div>
                </div>
              </div>
            </a>
            <% } %>
            </div>
          </div>
        </div>
      </div>
      <div class="content-border">
        <div class="content">
          <div class="slide_show">

            <% if (Object.keys(container['media']).length > 0) { %>
              <img id="first-img" src="<%= project_container.media[0].PRO_MUL_Link %>" onerror="this.onerror=null;this.src='../assets/img placeholder.svg';"/>
              <% } else { %>
                <img id="first-img" src="../assets/img placeholder.svg" />
              <% } %>
  
              <% if (Object.keys(container['media']).length > 1) { %>
                <img id="second-img" src="<%= container.media[1].BID_ATT_Link %>" onerror="this.onerror=null;this.src='../assets/img placeholder.svg';"/>
                <% }else { %>
                  <img id="second-img" src="../assets/img placeholder.svg" />
                <% } %>

          </div>
          <div class="project-info">
            <div id="logo-bar">
              <img onerror="this.onerror=null;this.src='../assets/img placeholder.svg';" src="<%= project_container.project.PRO_Thumbnail %>" />
            </div>
            <div id="info-bar">
              <div id="project-title">
                <h1><%= project_container.project.PRO_Name %></h1>
                <span>
                  <%= project_container.project.PRO_Start_Date %> - <%= project_container.project.PRO_End_Date %>
            </span>
              </div>
              <div id="rate-bar">
                <div id="rate">
                  <span style="filter: hue-rotate(<%= (project_container.project.PRO_Rate) * 30 %>deg) saturate(1.3) brightness(1.2);"><%= project_container.project.PRO_Rate %>

                  <% for(i = 0; i < project_container.project.PRO_Rate; i++) {%>
                    <svg height="15pt" viewBox="0 -10 511.98685 511" width="15pt" xmlns="http://www.w3.org/2000/svg"><path d="m510.652344 185.902344c-3.351563-10.367188-12.546875-17.730469-23.425782-18.710938l-147.773437-13.417968-58.433594-136.769532c-4.308593-10.023437-14.121093-16.511718-25.023437-16.511718s-20.714844 6.488281-25.023438 16.535156l-58.433594 136.746094-147.796874 13.417968c-10.859376 1.003906-20.03125 8.34375-23.402344 18.710938-3.371094 10.367187-.257813 21.738281 7.957031 28.90625l111.699219 97.960937-32.9375 145.089844c-2.410156 10.667969 1.730468 21.695313 10.582031 28.09375 4.757813 3.4375 10.324219 5.1875 15.9375 5.1875 4.839844 0 9.640625-1.304687 13.949219-3.882813l127.46875-76.183593 127.421875 76.183593c9.324219 5.609376 21.078125 5.097657 29.910156-1.304687 8.855469-6.417969 12.992187-17.449219 10.582031-28.09375l-32.9375-145.089844 111.699219-97.941406c8.214844-7.1875 11.351563-18.539063 7.980469-28.925781zm0 0" fill="#ff5147"/></svg>
                  <% } %>

                    <% for(i = 5 - project_container.project.PRO_Rate; i > 0; i--) {%>
                      <svg height="15pt" viewBox="0 -10 511.98685 511" width="15pt" xmlns="http://www.w3.org/2000/svg"><path d="m510.652344 185.902344c-3.351563-10.367188-12.546875-17.730469-23.425782-18.710938l-147.773437-13.417968-58.433594-136.769532c-4.308593-10.023437-14.121093-16.511718-25.023437-16.511718s-20.714844 6.488281-25.023438 16.535156l-58.433594 136.746094-147.796874 13.417968c-10.859376 1.003906-20.03125 8.34375-23.402344 18.710938-3.371094 10.367187-.257813 21.738281 7.957031 28.90625l111.699219 97.960937-32.9375 145.089844c-2.410156 10.667969 1.730468 21.695313 10.582031 28.09375 4.757813 3.4375 10.324219 5.1875 15.9375 5.1875 4.839844 0 9.640625-1.304687 13.949219-3.882813l127.46875-76.183593 127.421875 76.183593c9.324219 5.609376 21.078125 5.097657 29.910156-1.304687 8.855469-6.417969 12.992187-17.449219 10.582031-28.09375l-32.9375-145.089844 111.699219-97.941406c8.214844-7.1875 11.351563-18.539063 7.980469-28.925781zm0 0" fill="none" stroke="#ff5147" stroke-width="40px"/></svg>
                    <% }%>
                  </span>
                </div>
                <div id="categories-bar">
                  <div id="categories">
                    <% for(x in project_container.Fields) {%>
                      <p><%= project_container.Fields[x].PRO_CAT_Field %></p> &ensp;
                   <% } %>
                   
                  </div>
                  <span style="color: white;"> | </span>
                  <div id="skills">
                    <% for(x in project_container.Skills) {%>
                      <p><%= project_container.Skills[x].PRO_CAT_Skill %></p>  &ensp;
                   <% } %>
                  </div>
                </div>
              </div>
              <div id="discreption">
                <svg
                  id="dis-border"
                  width="157"
                  height="112"
                  viewBox="0 0 157 112"
                  fill="none"
                  xmlns="http://www.w3.org/2000/svg"
                >
