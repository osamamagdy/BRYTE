<%- include('partials/header'); -%>

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Protfolio</title>
</head>

<body>
  <div class="main">
    <div id="side-bar-container">
      <div class="side_bar">
        <div id="upper">
          <img class="profile-pic" src="<%= devCont.dev['DEV_Profile_Picture'] %>" />
          <div>
            <h1><%= devCont.dev['DEV_Fname'] %> <%= devCont.dev['DEV_Lname'] %></h1>
            <a href="#">
              <h5><%= devCont.dev['DEV_User_Name'] %></h5>
            </a>
          </div>
        </div>
        <div class="Side-Sheet">
          <svg id="side-bar-arrow" width="313" height="119" viewBox="0 0 313 119" fill="none"
