    <script>
              const Anims = {AFW: 65, JT: 46, PP: 46, VYS: 65};
              document.onload = () => {
                  for(x in Anims){
          for (let i = 0; i < Anims[x]+1; i++) {
              if (i < 10)
                  $('body').append(`<div id="preload-image-${i}" style="background-image: url('Morph Animations/${x}/${x}000${i}.png');"</div>`)
              else
                  $('body').append(`<div id="preload-image-${i}" style="background-image: url('Morph Animations/${x}/${x}00${i}.png');"</div>`)
          }
          ForwardAnimation(x);
        }
      };
      ForwardAnimation("PP");
      ForwardAnimation("AFW");
      ForwardAnimation("VYS");
      ForwardAnimation("JT");


      function ForwardAnimation(id) {
    const imagePath = 'Morph Animations/' + id + '/' + id + '00';
    const totalFrames = Anims[id];
    let timePerFrame = 0;
    if(totalFrames > 50)
        timePerFrame = 1200/totalFrames;
    else
        timePerFrame = 800/totalFrames;    
    const temptime = timePerFrame;
    let timeWhenLastUpdate;
    let timeFromLastUpdate;
    let length = document.querySelector("."+ id + "0000").src.length;
    let frameNumber = parseInt(document.querySelector("."+ id + "0000").src[length - 6] + document.querySelector("."+ id + "0000").src[length - 5]);

    function step(startTime) {
        if (!timeWhenLastUpdate) timeWhenLastUpdate = startTime;

        timeFromLastUpdate = startTime - timeWhenLastUpdate;

        if (timeFromLastUpdate > timePerFrame) {
            if (frameNumber < 10)
                document.querySelectorAll("."+ id + "0000").forEach((el) => {el.setAttribute('src', imagePath + `0${frameNumber}.png`);});
            else
            document.querySelectorAll("."+ id + "0000").forEach((el) => {el.setAttribute('src', imagePath + `${frameNumber}.png`);});
            timeWhenLastUpdate = startTime;

            if (frameNumber >= totalFrames) {
                BackwardAnimation(id);
                return;
            } else {
                frameNumber = frameNumber + 1;
                if(frameNumber < 3 || totalFrames - frameNumber < 3)
                    timePerFrame = temptime * 3;
                else
                    timePerFrame = temptime;   
            }
        }
        requestAnimationFrame(step);

    }
    step(0);

}

function BackwardAnimation(id) {
    const imagePath = 'Morph Animations/' + id + '/' + id + '00';
    const totalFrames = Anims[id];
    let timePerFrame = 0;
    if(totalFrames > 50)
        timePerFrame = 1200/totalFrames;
    else
        timePerFrame = 800/totalFrames;    
    const temptime = timePerFrame;
    let timeWhenLastUpdate;
    let timeFromLastUpdate;
    let length = document.querySelector("."+ id + "0000").src.length;
    let frameNumber = parseInt(document.querySelector("."+ id + "0000").src[length - 6] + document.querySelector("."+ id + "0000").src[length - 5]);

    function step(startTime) {
        if (!timeWhenLastUpdate) timeWhenLastUpdate = startTime;

        timeFromLastUpdate = startTime - timeWhenLastUpdate;

        if (timeFromLastUpdate > timePerFrame) {
            if (frameNumber < 10)
                document.querySelectorAll("."+ id + "0000").forEach((el) => {el.setAttribute('src', imagePath + `0${frameNumber}.png`);});
            else
            document.querySelectorAll("."+ id + "0000").forEach((el) => {el.setAttribute('src', imagePath + `${frameNumber}.png`);});
            timeWhenLastUpdate = startTime;

            if (frameNumber <= 0) {
                ForwardAnimation(id);
                return;
            } else {
                frameNumber = frameNumber - 1;
                if(frameNumber < 3 || totalFrames - frameNumber < 3)
                    timePerFrame = temptime * 3;
                else
                    timePerFrame = temptime; 
            }
        }

        requestAnimationFrame(step);
    }

    step(0);

}
    </script>
