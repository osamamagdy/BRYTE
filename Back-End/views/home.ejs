            ><img class="AFW0000"
              src="Morph Animations\AFW\AFW0000.png"
              alt=""
            /><span id="info">Apply for Work</span></span
          >
        </div>
      </div>
    </div>
    <div id="section3">
      <div id="projectsList">
        <% for(x in projects) { %>
          <a href="/project/<%= projects[x].PRO_ID %>">
        <div class="ProjectInst">
            <img
            src="<%= projects[x].PRO_Thumbnail%>"
            class="ListMainPic"
            alt=""
          />
          <div class="details">
            <div class="project-head">
              <img class="project-avatar" src="assets/logo.jpg" />
              <div class="projecter-cont">
                <div class="project-title"><%= projects[x].PRO_Name %></div>
                <div style="font-family: sans-serif">
                  <span>
                    <%= projects[x].PRO_Rate %>
                    <svg
                      width="17"
                      height="15"
                      viewBox="0 0 17 15"
                      fill="none"
                      xmlns="http://www.w3.org/2000/svg"
                    >
                      <path
                        d="M8.55193 6.10352e-05L10.3652 5.52793H16.2331L11.4859 8.94433L13.2992 14.4722L8.55193 11.0558L3.8047 14.4722L5.61798 8.94433L0.870754 5.52793H6.73865L8.55193 6.10352e-05Z"
                        fill="#4B90F7"
                      />
                    </svg>
                  </span> <% if(projects[x].developers[0].DEV_Fname){ %>
                  <span 
                    >| by:<a href="" style="color: #66a3ff"><%= projects[x].developers[0].DEV_Fname %> <%= projects[x].developers[0].DEV_Lname %> </a></span
                  > <% } %>
                </div> 
              </div>
              <div>
                <div class="project-date"><%= projects[x].PRO_End_Date %></div>
                <div class="icons">
                  <div class="ProFields">
                    <% for(y in projects[x].categories) { %>
                      <img class="cat" src="..\assets\Fields\<%= projects[x].categories[y].PRO_CAT_Field%>.png" alt="">
                  <% } %>

                  </div>
                  <div class="ProSkills">
                    <% for(y in projects[x].categories) { %>
                      <img class="cat" src="..\assets\Skills\<%= projects[x].categories[y].PRO_CAT_Skill%>.png" alt="">                     
                  <% } %>
                  </div>
                </div>
              </div>
            </div>
            <div class="project-tail">
              <%= projects[x].PRO_Description %>
            </div>
          </div>
        </div>
        
        </a>
        <% } %>
      </div>
      <div id="ProRight">
